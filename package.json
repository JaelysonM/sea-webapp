{
  "name": "siasd-webapp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "tsc": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts,.json .",
    "lint:fix": "eslint --fix --ext .tsx,.ts,.json .",
    "format": "prettier --no-error-on-unmatched-pattern --write src/**/*.{ts,tsx,css,md,json,scss} --config ./.prettierrc",
    "test": "NODE_ENV=test jest",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^1.7.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@swc/core": "^1.3.92",
    "@swc/jest": "^0.2.29",
    "@tanstack/react-query": "^5.17.9",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "framer-motion": "^12.1.0",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.2.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.30.1",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.2.2",
    "react-icons": "^5.4.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "sass-loader": "^13.3.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@cypress/code-coverage": "^3.12.3",
    "@cypress/react18": "^2.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.4",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.14.198",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.15",
    "@types/react-bootstrap": "^1.1.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-jest": "^29.7.0",
    "cypress": "^13.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "start-server-and-test": "^2.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-istanbul": "^5.0.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
